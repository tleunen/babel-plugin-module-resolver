{
  "name": "babel-plugin-module-alias",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.js",
  "description": "Babel plugin to rewrite the path in require() and ES6 import",
  "repository": {
    "type": "git",
    "url": "https://github.com/tleunen/babel-plugin-module-alias.git"
  },
  "author": {
    "name": "Tommy Leunen",
    "email": "tommy.leunen@gmail.com",
    "url": "http://tommyleunen.com"
  },
  "license": "MIT",
  "keywords": [
    "babel",
    "babel-plugin",
    "module",
    "alias",
    "rewrite",
    "resolve",
    "rename",
    "mapping",
    "require",
    "import"
  ],
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "condition-node-version": "^1.3.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb-base": "^1.0.4",
    "eslint-plugin-import": "^1.6.0",
    "mocha": "^2.4.5",
    "semantic-release": "^4.3.5"
  },
  "release": {
    "verifyConditions": {
      "path": "condition-node-version",
      "node": ">=6.0.0",
      "verbose": true
    }
  },
  "scripts": {
    "lint": "eslint src test",
    "compile": "babel src --out-dir lib",
    "pretest": "npm run lint",
    "test": "mocha --compilers js:babel-core/register",
    "prepublish": "npm run compile",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  }
}
